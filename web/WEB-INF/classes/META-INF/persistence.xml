<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
	<persistence-unit name="CuduPersistenceUnit" transaction-type="RESOURCE_LOCAL">
		<class>org.scoutsfev.cudu.domain.Grupo</class>
        <class>org.scoutsfev.cudu.domain.Usuario</class>
        <class>org.scoutsfev.cudu.domain.Asociado</class>
        <class>org.scoutsfev.cudu.domain.AsociadoRama</class>
		<properties>
			<property name="hibernate.show_sql" value="true" />
			<property name="hibernate.format_sql" value="true" />
		</properties>			
	</persistence-unit>
</persistence>
